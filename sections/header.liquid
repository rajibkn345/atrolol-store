    },
    {
      "type": "header",
      "content": "t:general.mobile.name"
    },
    {
      "type": "checkbox",
      "id": "show_social_media_icons",
      "label": "t:sections.header.settings.show_social_media_icons.label",
      "default": true
    },
    {
      "type": "checkbox",
      "id": "enable_language_selector",
