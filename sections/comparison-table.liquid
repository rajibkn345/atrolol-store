{{ 'section-comparison-table.css' | asset_url | stylesheet_tag }}
{%- liquid
  assign header_layout = section.settings.header_layout
  assign side_button = true
  if header_layout == 'standing-column' or header_layout == 'vertical'
    assign header_layout = 'vertical'
    assign side_button = false
  endif

  assign heading_block = section.blocks | find: 'type', 'heading'
  assign content_block = section.blocks | where: 'type', 'content'

  assign featured_media = product.featured_media
  if featured_media.media_type == 'model' or featured_media.media_type == 'video'
    assign featured_media = product.featured_media.preview_image
  endif
-%}
{%- render 'divider',
  show_divider: section.settings.show_section_divider,
  divider_width: section.settings.divider_width
-%}
<div
  class="section section--padding page-width page-width--{{ section.settings.container }}  section-header-{{ section.settings.header_layout }} color-{{ section.settings.color_scheme }}"
  style="--section-padding-top: {{ section.settings.padding_top }}px; --section-padding-bottom: {{ section.settings.padding_bottom }}px;"
>
  <comparison-table class="block comparison-table quick-comparison-table-container">
    <div class="comparison-table__header">
      {%- render 'section-heading',
        section_settings: section.settings,
        header_layout: section.settings.header_layout,
        button_label: section.settings.button_label,
        button_link: section.settings.button_link,
        button_style: section.settings.button_style,
        button_icon: section.settings.button_icon,
        button_visible: true,
        side_button: side_button,
        custom_css_class: 'sticky-element'
      -%}
    </div>
    <div class="comparison-table__content h-scrollable relative z-0">
      <table class="quick-comparison-table text-subtext">
        {% comment %} Row with Product Images {% endcomment %}
        <tr>
          <td class="td-label background-1 text-body"></td>
          {% for block in content_block %}
            {% assign product = block.settings.product %}
            {% assign featured_media = product.featured_media %}
            <td class="comparison-table__product">
              <div class="product-featured-image relative">
                <div class="media-wrapper hover-wrapper blocks-radius" style="--aspect-ratio: 1;">
                  {% if featured_media %}
                    {{
                      featured_media
                      | image_url: width: featured_media.width
                      | image_tag:
                        loading: 'lazy',
                        fetchpriority: 'low',
                        class: 'motion-reduce hover-scale-up',
                        widths: '60, 70, 80, 100, 120, 140, 160, 240, 320, 450,',
                        sizes: "450px",
                        is: 'image-lazy'
                    }}
                  {% else %}
                    {{ 'product-1' | placeholder_svg_tag: 'placeholder-svg w-full h-full hover-scale-up' }}
                  {% endif %}
                </div>
              </div>
              <div class="comparison-table__col-info text-center">
                <h3 class="text-pcard-title">
                  <a class="reversed-link block" href="{{ product.url }}">
                    {% if product != blank %}
                      <span class="reversed-link__text">{{ product.title }}</span>
                    {% else %}
                      <span class="reversed-link__text">{{- 'onboarding.product_title' | t -}}</span>
                    {% endif %}
                  </a>
                </h3>
                <a href="{{ product.url }}" class="btn btn--primary btn--small">
                  <span class="btn__text flex gap-1 items-center">{{- 'products.product.shop' | t -}}</span>
                </a>
              </div>
            </td>
          {% endfor %}
        </tr>

        {% comment %} Dynamic Feature Rows {% endcomment %}
        {% for i in (1..10) %}
          {%- liquid
            assign title_key = 'title-' | append: i
            assign text_key = 'text-' | append: i
            assign has_content = false
            if heading_block.settings[title_key] != blank
              assign has_content = true
            endif
          -%}
          {% if has_content %}
            <tr>
              {% comment %} Heading Column {% endcomment %}
              {% assign title = heading_block.settings[title_key] %}
              {% assign icon_key = 'icon-' | append: i %}
              {% assign image_key = 'image-' | append: i %}
              {% assign icon = heading_block.settings[icon_key] %}
              {% assign image = heading_block.settings[image_key] %}

              <td class="td-label background-1 text-body">
                {% if title != blank %}
                  <div
                    class="comparison-table-card comparison-table-card--image-icon flex items-center gap-x-3 h-full"
                    style="--img-max: {{ section.settings.icon_size }}px;"
                    {{ heading_block.shopify_attributes }}
                  >
                    <div class="comparison-table-card__image shrink-0 hidden md:block">
                      <motion-element
                        class="block w-full h-full"
                        data-motion="zoom-out-sm"
                        style="--aspect-ratio: 1/1;"
                      >
                        {% if image %}
                          {% assign alt = image.alt | default: title | escape %}
                          <picture class="card-media__media media-wrapper">
                            {{
                              image
                              | image_url: width: 1100
                              | image_tag:
                                loading: 'lazy',
                                class: 'motion-reduce absolute inset-0 w-full h-full',
                                widths: '165, 360, 535, 750, 940, 1100',
                                is: 'image-lazy',
                                alt: alt
                            }}
                          </picture>
                        {% elsif icon != 'none' %}
                          {% render 'icons', icon: icon %}
                        {% else %}
                          {{ 'image' | placeholder_svg_tag: 'placeholder-svg blocks-radius-sm' }}
                        {% endif %}
                      </motion-element>
                    </div>
                    <div class="comparison-table__info rich-text rich-text--tight flex-1">
                      <div class="multicolumn-card__title rich-text__heading text-body">
                        <span class="label ml-2">{{ title }}</span>
                      </div>
                    </div>
                  </div>
                {% endif %}
              </td>

              {% comment %} Content Columns  {% endcomment %}
              {% for block in content_block %}
                {% assign text = block.settings[text_key] %}
                <td>
                  {% comment %} <div class="rte text-limit-{{ section.settings.text_line_limit }}">{{ text }}</div> {% endcomment %}
                  {%- if text != blank -%}
                    {%- if section.settings.show_view_more_button and section.settings.text_line_limit != 'none' -%}
                      <read-more
                        class="read-more__text"
                        data-line-clamp="{{ section.settings.text_line_limit }}"
                        data-toggle-class="text-limit-{{ section.settings.text_line_limit }}"
                        data-show-text="{{ 'accessibility.view_more' | t }}"
                        data-hide-text="{{ 'accessibility.view_less' | t }}"
                      >
                        <div class="read-more__content rte">
                          {{- text -}}
                        </div>
                        <button class="read-more__toggle btn btn--underline">
                          <span class="btn__text">{{ 'accessibility.view_more' | t }}</span>
                        </button>
                      </read-more>
                    {%- else -%}
                      <div class="rte text-limit-{{ section.settings.text_line_limit }}">
                        {{- text -}}
                      </div>
                    {%- endif -%}
                  {%- endif -%}
                </td>
              {% endfor %}
            </tr>
          {% endif %}
        {% endfor %}
      </table>
    </div>
  </comparison-table>
</div>
