{%- liquid
  assign media_layout = section.settings.gallery_layout | default: 'carousel'
  assign mobile_thumbnails = section.settings.mobile_thumbnails | default: 'hide'
  assign enable_image_zoom = section.settings.enable_image_zoom | default: false
  assign enable_sticky_info = section.settings.enable_sticky_info

  assign product_class = 'product--mobile-thumbs-' | append: mobile_thumbnails
  assign product_class = product_class | append: ' product--' | append: section.settings.media_size
  assign product_class = product_class | append: ' product--' | append: media_layout
  assign product_class = product_class | append: ' product--media-has-' | append: product.media.size
  if product.media.size > 1
    assign product_class = product_class | append: ' product--media-has-many'
  endif
  if section.settings.show_image_border
    assign product_class = product_class | append: ' product--media-has-border'
  endif

  case media_layout
    when 'grid-mix'
      assign media_modulo = product.media.size | modulo: 3
      if media_modulo == 2
        assign product_class = product_class | append: ' product--odd-media'
      endif
  endcase

  if section.settings.use_color_scheme_in_container
    assign product_class = product_class | append: ' theme-radius color-' | append: section.settings.color_scheme
  endif

  assign product_form_id = 'product-form-' | append: section.id
-%}
{%- style -%}
  .section-{{ section.id }}{
    --section-padding-top: {{ section.settings.padding_top }}px;
    --section-padding-bottom: {{ section.settings.padding_bottom }}px;
  }
{%- endstyle -%}
<product-info
  id="MainProduct-{{ section.id }}"
  data-section="{{ section.id }}"
  data-product-id="{{ product.id }}"
  data-update-url="true"
  data-url="{{ product.url }}"
  class="section block section-{{ section.id }} section-main-product page-width section--padding{% unless section.settings.use_color_scheme_in_container %} color-{{ section.settings.color_scheme }}{% else %} use-color-scheme-in-container{% endunless %}"
>
  {{ 'section-main-product.css' | asset_url | stylesheet_tag: preload: true }}
  {{ 'component-deferred-media.css' | asset_url | stylesheet_tag: preload: true }}
  {% unless product.has_only_default_variant %}
    {{ 'component-product-variant-picker.css' | asset_url | stylesheet_tag: preload: true }}
  {% endunless %}
  {%- assign has_models = product.media | where: 'media_type', 'model' | first -%}
  {%- if has_models -%}
    <link
      id="ModelViewerStyle"
      rel="stylesheet"
      href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css"
      media="print"
      onload="this.media='all'"
    >
    <link
      id="ModelViewerOverride"
      rel="stylesheet"
      href="{{ 'component-model-viewer-ui.css' | asset_url }}"
      media="print"
      onload="this.media='all'"
    >
  {%- endif -%}
  {%- if enable_image_zoom -%}
    <link
      id="Photoswipe"
      rel="stylesheet"
      href="{{ 'photoswipe-component.css' | asset_url }}"
      media="print"
      onload="this.media='all'"
    >
  {%- endif -%}

  {%- if product.quantity_price_breaks_configured? -%}
    {{ 'component-volume-pricing.css' | asset_url | stylesheet_tag }}
    <script src="{{ 'show-more.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'price-per-item.js' | asset_url }}" defer="defer"></script>
  {%- endif -%}

  <script src="{{ 'product-info.js' | asset_url }}" defer="defer"></script>
  <script src="{{ 'variant-vat-update.js' | asset_url }}" defer="defer"></script>

  {% assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src' %}
  <div class="product flex flex-wrap {{ product_class }}">
    {%- if product.media.size > 0 -%}
      <div class="product__media-wrapper block w-full relative">
        <div class="product__media-container{% if enable_sticky_info %} sticky-element{% endif %}">
          {% render 'product-media-gallery',
            media_layout: media_layout,
            mobile_thumbnails: mobile_thumbnails,
            variant_images: variant_images,
            enable_image_zoom: enable_image_zoom
          %}

          {% render 'product-information-below-media', product: product %}
        </div>
      </div>
    {%- endif -%}
    <div class="product__info-wrapper block w-full">
      {% render 'product-information-blocks',
        enable_sticky_info: enable_sticky_info,
        update_browser_history: true,
        product_form_id: product_form_id
      %}
    </div>
  </div>
  <product-recently-viewed data-product-id="{{ product.id }}" class="hidden"></product-recently-viewed>
  {%- if enable_image_zoom -%}
    <script src="{{ 'photoswipe.js' | asset_url }}" defer="defer"></script>
  {%- endif -%}

  {%- if product.media.size > 0 -%}
    <script src="{{ 'media-gallery.js' | asset_url }}" defer="defer"></script>
  {%- endif -%}

  {%- if has_models -%}
    <script type="application/json" id="ProductJSON-{{ product.id }}">
      {{ product.media | where: 'media_type', 'model' | json }}
    </script>
    <script src="{{ 'product-model.js' | asset_url }}" defer></script>
  {%- endif -%}

  <script type="application/ld+json">
    {{ product | structured_data }}
  </script>
</product-info>
{%- assign block_atc_bar = section.blocks | where: 'type', 'sticky-atc-bar' | first -%}
{%- if block_atc_bar != blank -%}
  {%- render 'sticky-atc-bar', block: block_atc_bar, product_form_id: product_form_id -%}
{%- endif -%}
